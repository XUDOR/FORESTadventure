# Ignore bundler config
/.bundle

# Ignore the default SQLite database
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more
/config/master.key

# Ignore node_modules
/node_modules/

# Ignore precompiled JavaScript packs
/public/packs
/public/packs-test
/public/packs-test/

# Ignore yarn files
/yarn-error.log
yarn-debug.log*

# Ignore .env files
.env
.env.*

# Ignore IDE and text editor files
/.idea/
*.swp
*.swo
.vscode/

# Ignore OS files
.DS_Store
Thumbs.db
